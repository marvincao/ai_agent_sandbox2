{
    "svg.preview.background": "editor",
    "terminal.integrated.persistentHistory.size": 10000,
    "terminal.integrated.defaultProfile.windows": "Command Prompt",
    "terminal.integrated.profiles.windows": {
        "PowerShell": {
            "source": "PowerShell",
            "icon": "terminal-powershell"
        },
        "Command Prompt": {
            "path": [
                "${env:windir}\\Sysnative\\cmd.exe",
                "${env:windir}\\System32\\cmd.exe"
            ],
            "args": [],
            "icon": "terminal-cmd"
        },
        "Git Bash": {
            "source": "Git Bash",
            "args": ["--cd-to-home"]
        },
    },
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "entity.name.function.decorator.python",
                "settings": {
                    "foreground": "#FF6347"
                }
            },
        ]
    },
    "markdownlint.config": {
        "MD041": false,
        "MD029": false,
        "MD032": false,
    },
    "yaml.schemas": {
        "https://raw.githubusercontent.com/kedro-org/kedro/develop/static/jsonschema/kedro-catalog-0.19.json": [
            "conf/**/*catalog*",
            "file:///c%3A/Users/minca/code/src/python/heb-2019/kedro-cfm/conf/base/catalog.yml"
        ]
    }
}
